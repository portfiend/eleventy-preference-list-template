{% set links = {
	"Home": "/",
	"Profiles": "/profiles/",
	"Download": "/download/"
} %}
<nav>
	
	<ul>
		{% for key, value in links %}
			<li>
				<a href="{{value}}" {{ page.url === value and 'aria-current=page' or "" }}>
					{{key}}
				</a>
			</li>
		{% endfor %}
		{% if options.enableContentWarnings %}
			<li>
				<a href="#" id="cw-dialogue">Content Options</a>
			</li>
		{% endif %}
	</ul>
</nav>